   "source": [
    "n = int(input())\n",
    "times = list(map(int, input().split()))\n",
    "\n",
    "times.sort()\n",
    "ans = 0\n",
    "# 2. 시간이 적게 드는 영역을 앞에 둬서 더하기\n",
    "for i in range(len(times)): \n",
    "    ans += times[i] * (n - i)\n",
    "\n",
    "print(ans)\n"
   ]
  }
