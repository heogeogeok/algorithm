   "source": [
    "n, k = map(int, input().split())\n",
    "items = [tuple(map(int, input().split())) for _ in range(n)]\n",
    "\n",
    "dp = [0] * (k + 1)\n",
    "for weight, value in items:\n",
    "    for w in range(k, weight-1, -1):\n",
    "        dp[w] = max(dp[w], dp[w - weight] + value)\n",
    "\n",
    "print(dp[k])"
   ]
  }
