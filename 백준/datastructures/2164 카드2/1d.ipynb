   "source": [
    "from collections import deque\n",
    "n = int(input())\n",
    "cards = deque(i for i in range(1, n + 1))\n",
    "\n",
    "while True:\n",
    "    if len(cards) == 1:\n",
    "        print(cards[0])\n",
    "        break\n",
    "    cards.popleft()\n",
    "    cards.append(cards.popleft())"
   ]
  }
