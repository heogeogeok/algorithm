   "source": [
    "n = int(input())\n",
    "dp = [0] * (n + 1)\n",
    "for i in range(2, n + 1):\n",
    "    candidates = []\n",
    "    if i % 3 == 0:\n",
    "        candidates.append(1 + dp[i // 3])\n",
    "    if i % 2 == 0:\n",
    "        candidates.append(1 + dp[i // 2])\n",
    "    candidates.append(1 + dp[i - 1])\n",
    "\n",
    "    dp[i] = min(candidates)\n",
    "print(dp[n])"
   ]
  }
