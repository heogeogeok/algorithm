   "source": [
    "def is_palidrome(s):\n",
    "    if len(s) <= 1:\n",
    "        return 1\n",
    "    if s[0] != s[-1]:\n",
    "        return 0\n",
    "    return is_palidrome(s[1:-1])"
   ]
