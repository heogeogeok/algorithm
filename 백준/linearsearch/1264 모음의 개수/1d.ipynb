   "source": [
    "while True:\n",
    "    # 1. 입력 처리\n",
    "    sentence = input()\n",
    "    answer = 0\n",
    "    if sentence == '#':\n",
    "        break\n",
    "    # 2. 문장 순회\n",
    "    for s in sentence:\n",
    "        if s in ['a', 'e', 'i', 'o', 'u', 'A', 'E', 'I', 'O', 'U']:\n",
    "            answer += 1\n",
    "    # 3. 모음 개수 출력\n",
    "    print(answer)"
   ]
  }
